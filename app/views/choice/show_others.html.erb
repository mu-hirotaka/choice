<div class="page-header-my">
  <h3>その他</h3>
</div>

<div class="my-table">
  <table class="table">
    <thead><tr><th>順位</th><th>Name</th><th>選択回数</th></tr></thead>
    <tbody>
      <% rank = 1 %>
      <% @summaries.each do |record| %>
        <tr>
          <td><%= rank %></td>
          <td><%= @chara_info[record.character_id] %></td>
          <td><%= record.selected_count %></td>
        </tr>
        <% rank = rank + 1 %>
      <% end %>
    </tbody>
  </table>
</div>
